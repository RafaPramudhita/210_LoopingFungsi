#include <iostream>
using namespace std;

int main()
{
    int bilangan;

    srand(time(0));

    bilangan = rand() % 10;

    do 
    {
        cout << "Perulangan dengan DO..WHILE" << ENDL;
        bilangan = rand() % 10;
        cout << "Bilangannya = " << bilangan << endl;
    } while (bilangan > 4);